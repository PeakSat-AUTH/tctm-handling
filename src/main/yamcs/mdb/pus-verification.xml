<?xml version="1.0" encoding="UTF-8"?>
<xtce:SpaceSystem name="pus-verification" xmlns:xtce="http://www.omg.org/spec/XTCE/20180204" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.omg.org/spec/XTCE/20180204 https://www.omg.org/spec/XTCE/20180204/SpaceSystem.xsd" shortDescription="This is a bogus satellite telemetry and telecommand database." operationalStatus="unittest">

    <!-- Request Verification Service (ST[01]) TM and TC -->
    <xtce:TelemetryMetaData>
        <xtce:ParameterSet>
            <xtce:Parameter parameterTypeRef="/dt/packet_version_number" name="source_packet_version_number"></xtce:Parameter>
            <xtce:Parameter parameterTypeRef="/dt/packet_type" name="source_packet_type"></xtce:Parameter>
            <xtce:Parameter parameterTypeRef="/dt/secondary_header_flag" name="source_secondary_header_flag"></xtce:Parameter>
            <xtce:Parameter parameterTypeRef="/dt/application_process_ID" name="source_application_process_ID"></xtce:Parameter>
            <xtce:Parameter parameterTypeRef="/dt/sequence_flags" name="source_sequence_flags"></xtce:Parameter>
            <xtce:Parameter parameterTypeRef="/dt/packet_sequence_count_or_packet_name" name="source_packet_sequence_count"></xtce:Parameter>
            <xtce:Parameter parameterTypeRef="/dt/AcceptanceErrorType_t" name="AcceptanceErrorType"></xtce:Parameter>
            <xtce:Parameter parameterTypeRef="/dt/ExecutionStartErrorType_t" name="ExecutionStartErrorType"></xtce:Parameter>
            <xtce:Parameter parameterTypeRef="/dt/uint8_t" name="stepID"></xtce:Parameter>
            <xtce:Parameter parameterTypeRef="/dt/ExecutionProgressErrorType_t" name="ExecutionProgressErrorType"></xtce:Parameter>
            <xtce:Parameter parameterTypeRef="/dt/ExecutionCompletionErrorType_t" name="ExecutionCompletionErrorType"></xtce:Parameter>
            <xtce:Parameter parameterTypeRef="/dt/RoutingErrorType_t" name="RoutingErrorType"></xtce:Parameter>
        </xtce:ParameterSet>

        <xtce:ContainerSet>
            <xtce:SequenceContainer name="verification_data">
